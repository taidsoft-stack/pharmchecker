        }
      });
      
      if (!isValid) {
        alert('ëª¨ë“  í•„ìˆ˜ í•­ëª©ì„ ì…ë ¥í•´ì£¼ì„¸ìš”.');
        if (firstErrorField) {
          firstErrorField.focus();
        }
        return;
      }
      
      // í•¸ë“œí° ë²ˆí˜¸ ìœ íš¨ì„± ê²€ì¦
      const phonePattern = /^01[0-9]-\d{3,4}-\d{4}$/;
      const pharmacistPhone = document.getElementById('pharmacistPhone').value;
      if (!phonePattern.test(pharmacistPhone)) {
        alert('ì˜¬ë°”ë¥¸ í•¸ë“œí° ë²ˆí˜¸ í˜•ì‹ì´ ì•„ë‹™ë‹ˆë‹¤ (ì˜ˆ: 010-1234-5678)');
        document.getElementById('pharmacistPhone').classList.add('error');
        document.getElementById('pharmacistPhone').focus();
        return;
      }
      
      // ì‚¬ì—…ì ë²ˆí˜¸ ìœ íš¨ì„± ê²€ì¦ (10ìë¦¬)
      const businessNumber = document.getElementById('businessNumber').value.replace(/[^0-9]/g, '');
      if (businessNumber.length !== 10) {
        alert('ì‚¬ì—…ì ë²ˆí˜¸ëŠ” 10ìë¦¬ì—¬ì•¼ í•©ë‹ˆë‹¤ (ì˜ˆ: 123-45-67890)');
        document.getElementById('businessNumber').classList.add('error');
        document.getElementById('businessNumber').focus();
        return;
      }
      
      // í¼ ë°ì´í„° ìˆ˜ì§‘
      const formData = {
