    // í•¸ë“œí° ë²ˆí˜¸ ìë™ í•˜ì´í”ˆ ì¶”ê°€ ë° ìœ íš¨ì„± ê²€ì¦
    function formatPhoneNumber(input) {
      let value = input.value.replace(/[^0-9]/g, ''); // ìˆ«ìë§Œ ì¶”ì¶œ
      let formatted = '';
      
      if (value.length <= 3) {
        formatted = value;
      } else if (value.length <= 7) {
        formatted = value.substr(0, 3) + '-' + value.substr(3);
      } else if (value.length <= 11) {
        formatted = value.substr(0, 3) + '-' + value.substr(3, 4) + '-' + value.substr(7);
      } else {
        formatted = value.substr(0, 3) + '-' + value.substr(3, 4) + '-' + value.substr(7, 4);
      }
      
      input.value = formatted;
      
      // ìœ íš¨ì„± ê²€ì¦
      const phonePattern = /^01[0-9]-\d{3,4}-\d{4}$/;
      if (formatted.length >= 12 && !phonePattern.test(formatted)) {
        input.setCustomValidity('ì˜¬ë°”ë¥¸ í•¸ë“œí° ë²ˆí˜¸ í˜•ì‹ì´ ì•„ë‹™ë‹ˆë‹¤ (010-1234-5678)');
      } else {
        input.setCustomValidity('');
      }
    }
    
    // ì¼ë°˜ ì „í™”ë²ˆí˜¸ ìë™ í•˜ì´í”ˆ ì¶”ê°€
    function formatTelNumber(input) {
      let value = input.value.replace(/[^0-9]/g, '');
      let formatted = '';
      
      // ì„œìš¸ 02
      if (value.startsWith('02')) {
        if (value.length <= 2) {
          formatted = value;
        } else if (value.length <= 5) {
          formatted = value.substr(0, 2) + '-' + value.substr(2);
        } else if (value.length <= 9) {
          formatted = value.substr(0, 2) + '-' + value.substr(2, 3) + '-' + value.substr(5);
        } else {
          formatted = value.substr(0, 2) + '-' + value.substr(2, 4) + '-' + value.substr(6, 4);
        }
      } 
      // ì§€ì—­ë²ˆí˜¸ 3ìë¦¬ (031, 032 ë“±)
      else {
        if (value.length <= 3) {
          formatted = value;
        } else if (value.length <= 6) {
          formatted = value.substr(0, 3) + '-' + value.substr(3);
        } else if (value.length <= 10) {
          formatted = value.substr(0, 3) + '-' + value.substr(3, 3) + '-' + value.substr(6);
        } else {
          formatted = value.substr(0, 3) + '-' + value.substr(3, 4) + '-' + value.substr(7, 4);
        }
      }
      
      input.value = formatted;
    }
    
    // ì‚¬ì—…ì ë²ˆí˜¸ ìë™ í•˜ì´í”ˆ ì¶”ê°€
