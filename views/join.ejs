      }
    }
    
    // JWT í† í° íŒŒì‹±
    function parseJwt(token) {
      const base64Url = token.split('.')[1];
      const base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
      const jsonPayload = decodeURIComponent(atob(base64).split('').map(function(c) {
        return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
      }).join(''));
      return JSON.parse(jsonPayload);
    }
    
    // ë‹¤ìŒ ì¹´ì¹´ì˜¤ ì£¼ì†Œ ê²€ìƒ‰ API
    function searchAddress() {
      new daum.Postcode({
        oncomplete: function(data) {
          // ë„ë¡œëª… ì£¼ì†Œì™€ ì§€ë²ˆ ì£¼ì†Œ ì¤‘ ì„ íƒí•œ ê²ƒ ì‚¬ìš©
          let addr = '';
          
          if (data.userSelectedType === 'R') {
            addr = data.roadAddress; // ë„ë¡œëª… ì£¼ì†Œ
          } else {
            addr = data.jibunAddress; // ì§€ë²ˆ ì£¼ì†Œ
          }
          
          // ìš°í¸ë²ˆí˜¸ì™€ ì£¼ì†Œ ì •ë³´ë¥¼ í•„ë“œì— ì…ë ¥
          document.getElementById('postcode').value = data.zonecode;
          document.getElementById('address').value = addr;
          
          // ìƒì„¸ì£¼ì†Œ ì…ë ¥ë€ìœ¼ë¡œ í¬ì»¤ìŠ¤ ì´ë™
          document.getElementById('detailAddress').focus();
